{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/fx-nx-prisma-stack-e2e/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nrwl/cypress:cypress",
      "options": {
        "cypressConfig": "apps/fx-nx-prisma-stack-e2e/cypress.json",
        "devServerTarget": "fx-nx-prisma-stack:serve:development"
      },
      "configurations": {
        "production": {
          "devServerTarget": "fx-nx-prisma-stack:serve:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/fx-nx-prisma-stack-e2e/**/*.{js,ts}"]
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["fx-nx-prisma-stack"]
}
