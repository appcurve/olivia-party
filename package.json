{
  "name": "fx-nx-prisma-stack",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "engines": {
    "npm": "please-use-yarn"
  },
  "prisma": {
    "seed": "ts-node apps/api/prisma/seed.ts",
    "schema": "apps/api/prisma/schema.prisma"
  },
  "scripts": {
    "start": "nx run-many --target=serve --projects=ui,api --parallel",
    "start:ui": "nx serve ui",
    "start:api": "nx serve api",
    "build": "nx build:all",
    "build:all": "nx run-many --target=build --projects=ui,api --parallel",
    "build:all:prod": "nx run-many --target=build --projects=ui,api --parallel --prod",
    "test:all": "nx run-many --target=test --all --parallel",
    "test:affected": "nx affected:test --all --codeCoverage --skip-nx-cache",
    "test:ui": "nx test ui",
    "test:api": "nx test api",
    "prebuild": "yarn prisma:generate",
    "prisma:generate": "yarn prisma generate --schema apps/api/prisma/schema.prisma",
    "prisma:migrate": "yarn prisma migrate --schema apps/api/prisma/schema.prisma",
    "prisma:studio": "yarn dotenv -e apps/api/.env yarn prisma studio --schema apps/api/prisma/schema.prisma",
    "prisma:db:seed": "yarn dotenv -e apps/api/.env yarn prisma db seed --schema apps/api/prisma/schema.prisma",
    "prisma:db:push": "yarn dotenv -e apps/api/.env yarn prisma db push --schema apps/api/prisma/schema.prisma",
    "docker:up": "yarn docker:postgres:up",
    "docker:down": "docker compose down",
    "docker:postgres:up": "docker compose up -d postgres && exit 0",
    "docker:postgres:down": "docker compose down postgres",
    "docker:postgres:cli": "docker exec -it postgres bash",
    "nx:migrate": "nx migrate latest"
  },
  "private": true,
  "dependencies": {
    "@headlessui/react": "^1.6.6",
    "@heroicons/react": "^1.0.6",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.1.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^8.0.1",
    "@nestjs/passport": "^8.2.2",
    "@nestjs/platform-express": "^9.0.7",
    "@prisma/client": "^3.15.2",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.4",
    "@tanstack/react-query": "^4.0.8",
    "@tanstack/react-query-devtools": "^4.0.10",
    "add": "^2.0.6",
    "argon2": "^0.28.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "clsx": "^1.2.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "core-js": "^3.6.5",
    "csurf": "^1.11.0",
    "dotenv": "^16.0.1",
    "dotenv-cli": "^5.1.0",
    "helmet": "^5.1.0",
    "next": "12",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "postcss": "^8.4.14",
    "react": "18.2.0",
    "react-async-hook": "^4.0.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.33.1",
    "react-use": "^17.4.0",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "0.13.7",
    "rxjs": "^7.0.0",
    "tailwindcss": "^3.1.6",
    "tslib": "^2.3.0",
    "yarn": "^1.22.19",
    "zod": "^3.17.9"
  },
  "devDependencies": {
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.4.7",
    "@nrwl/cli": "14.4.2",
    "@nrwl/cypress": "14.4.2",
    "@nrwl/eslint-plugin-nx": "14.4.2",
    "@nrwl/jest": "14.4.2",
    "@nrwl/linter": "14.4.2",
    "@nrwl/nest": "14.4.2",
    "@nrwl/next": "^14.4.2",
    "@nrwl/node": "14.4.2",
    "@nrwl/react": "14.4.2",
    "@nrwl/web": "14.4.2",
    "@nrwl/workspace": "14.4.2",
    "@testing-library/react": "13.3.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/csurf": "^1.11.2",
