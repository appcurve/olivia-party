{
  "name": "fx-nx-prisma-stack",
  "version": "0.0.0",
  "license": "MIT",
  "prisma": {
    "seed": "ts-node apps/api/prisma/seed.ts",
    "schema": "apps/api/prisma/schema.prisma"
  },
  "scripts": {
    "start": "nx run-many --target=serve --projects=ui,api --parallel",
    "start:ui": "nx serve ui",
    "start:api": "nx serve api",
    "build": "nx build",
    "test:all": "nx run-many --target=test --all --parallel",
    "test:affected": "nx affected:test --all --codeCoverage --skip-nx-cache",
    "test:ui": "nx test ui",
    "test:api": "nx test api",
    "prebuild": "yarn prisma:generate",
    "prisma:generate": "yarn prisma generate --schema apps/api/prisma/schema.prisma",
    "prisma:migrate": "yarn prisma migrate --schema apps/api/prisma/schema.prisma",
    "prisma:seed": "yarn dotenv -e apps/api/.env yarn prisma db seed --schema apps/api/prisma/schema.prisma",
    "prisma:studio": "yarn dotenv -e apps/api/.env yarn prisma studio --schema apps/api/prisma/schema.prisma",
    "prisma:db:push": "yarn dotenv -e apps/api/.env yarn prisma db push --schema apps/api/prisma/schema.prisma",
    "docker:postgres:up": "docker compose up -d postgres && exit 0",
    "docker:postgres:down": "docker compose down postgres",
    "docker:postgres:cli": "docker exec -it postgres bash",
    "docker:down": "docker compose down",
    "nx:migrate": "nx migrate latest"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@prisma/client": "^3.15.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "core-js": "^3.6.5",
    "dotenv": "^16.0.1",
    "dotenv-cli": "^5.1.0",
    "helmet": "^5.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "0.13.7",
    "rxjs": "^7.0.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@nrwl/cli": "14.3.6",
    "@nrwl/cypress": "14.3.6",
    "@nrwl/eslint-plugin-nx": "14.3.6",
    "@nrwl/jest": "14.3.6",
    "@nrwl/linter": "14.3.6",
    "@nrwl/nest": "^14.3.6",
    "@nrwl/node": "14.3.6",
    "@nrwl/react": "14.3.6",
    "@nrwl/web": "14.3.6",
    "@nrwl/workspace": "14.3.6",
    "@testing-library/react": "13.3.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.7",
    "@types/react": "18.0.13",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "~5.24.0",
    "@typescript-eslint/parser": "~5.24.0",
    "babel-jest": "27.5.1",
    "cypress": "^9.1.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "27.5.1",
    "nx": "14.3.6",
    "prettier": "^2.6.2",
    "prisma": "^3.15.2",
    "react-test-renderer": "18.2.0",
    "ts-jest": "27.1.4",
    "ts-node": "~10.8.0",
    "typescript": "~4.7.2"
  }
}
