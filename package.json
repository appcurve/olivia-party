{
  "name": "fx-nx-prisma-stack",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "private": true,
  "engines": {
    "npm": "please-use-yarn"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} apps/api/prisma/seed.ts",
    "schema": "apps/api/prisma/schema.prisma"
  },
  "scripts": {
    "git:cleanup": "git fetch -p && for branch in `git for-each-ref --format '%(refname) %(upstream:track)' refs/heads | awk '$2 == \"[gone]\" {sub(\"refs/heads/\", \"\", $1); print $1}'`; do git branch -D $branch; done",
    "start": "nx run-many --target=serve --projects=ui,api --parallel",
    "start:all": "nx run-many --target=serve --projects=player,ui,api --parallel",
    "start:ui:stack": "nx run-many --target=serve --projects=ui,api --parallel",
    "start:player:stack": "nx run-many --target=serve --projects=player,api --parallel",
    "start:player": "nx serve player",
    "start:ui": "nx serve ui",
    "start:api": "nx serve api",
    "lint": "yarn lint:all",
    "lint:all": "nx run-many --all --target=lint",
    "build": "yarn build:all",
    "build:all": "nx run-many --target=build --projects=player,ui,api --parallel",
    "build:all:prod": "NODE_ENV=production nx run-many --target=build --projects=player,ui,api --parallel --prod",
    "build:api": "nx run api:build",
    "build:api:prod": "NODE_ENV=production nx run api:build:production --prod",
    "build:ui": "nx run ui:build",
    "build:ui:prod": "NODE_ENV=production nx run ui:build:production --prod",
    "export:ui": "NODE_ENV=production nx export ui --prod",
    "export:ui:prod": "NODE_ENV=production nx run ui:build:production --prod && NODE_ENV=production nx export ui --prod",
    "build:player": "nx run player:build",
    "build:player:prod": "NODE_ENV=production nx run player:build:production --prod",
    "export:player": "NODE_ENV=production nx export player --prod",
    "export:player:prod": "NODE_ENV=production nx run player:build:production --prod && NODE_ENV=production nx export player --prod",
    "build:infra": "yarn nx run-many --target=build --projects=infra",
    "build:infra:prod": "yarn nx run-many --target=build --projects=infra --prod",
    "cdk:synth": "npx cdk synth",
    "cdk:deploy": "npx cdk deploy",
    "cdk:destroy": "npx cdk destroy",
    "test": "nx run-many --target=test --all --parallel",
    "test:all": "nx run-many --target=test --all --parallel",
    "test:affected": "nx affected:test --all --codeCoverage --skip-nx-cache",
    "test:ui": "nx test ui",
    "test:api": "nx test api",
    "test:player": "nx test player",
    "test:react-libs": "nx run-many --target=test --projects=shared-react-feedback,shared-react-forms-rhf,shared-react-hooks,shared-react-modals --parallel",
    "prebuild": "yarn prisma:generate",
    "prisma:generate": "yarn dotenv -e apps/api/.env yarn prisma generate --schema apps/api/prisma/schema.prisma && echo 'If you made schema changes you may need to restart the ts server and/or restart VSCode to refresh types.'",
    "prisma:migrate": "yarn dotenv -e apps/api/.env yarn prisma migrate --schema apps/api/prisma/schema.prisma",
    "prisma:migrate:dev": "yarn dotenv -e apps/api/.env yarn prisma migrate dev --schema apps/api/prisma/schema.prisma",
    "prisma:migrate:reset": "yarn dotenv -e apps/api/.env yarn prisma migrate reset --schema apps/api/prisma/schema.prisma",
    "prisma:studio": "yarn dotenv -e apps/api/.env yarn prisma studio --schema apps/api/prisma/schema.prisma",
    "prisma:seed": "yarn dotenv -e apps/api/.env yarn prisma db seed --schema apps/api/prisma/schema.prisma",
    "prisma:push": "yarn dotenv -e apps/api/.env yarn prisma db push --schema apps/api/prisma/schema.prisma",
    "docker:api:up": "docker compose up api",
    "docker:api:build:up": "docker compose up --build api",
    "docker:down": "docker compose down",
    "docker:postgres:up": "docker compose up -d postgres && exit 0",
    "docker:postgres:down": "docker compose down postgres",
    "docker:postgres:cli": "docker exec -it postgres bash",
    "nx:migrate": "nx migrate latest",
    "deploy:player": "yarn build:infra && yarn export:player:prod && yarn cdk:deploy PlayerStackProd && ./scripts/workflow/cloudfront-invalidate-cache.sh",
    "deploy:player:alt": "rsync -rcz --force --delete --progress -e \"ssh -p24816 -i ~/.ssh/id_rsa_insecure\" dist/apps/player/exported/* firxworx@server29.hostwhitelabel.com:/home/firxworx/projects.firxworx.com/olivia-joystick-nextjs && echo \"https://projects.firxworx.com/olivia-joystick-nextjs\""
  },
